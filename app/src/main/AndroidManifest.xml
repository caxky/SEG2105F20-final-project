<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.service_novigrad" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >

        <activity android:name=".ui.admin.AdminAccounts" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.register.RegisterActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".ui.welcome.WelcomeActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".ui.admin.AdminPanelActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".ui.admin.AdminServices"
            android:label="@string/app_name" />
        <activity
            android:name=".ui.services.ServiceItemActivity"
            android:label="@string/app_name"
            android:parentActivityName=".ui.admin.AdminServices" />
    </application>

</manifest>